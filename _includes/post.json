{
	"thumb"    : "{{ post.thumb }}",
	"author"    : "{{ post.author }}",
	"title"    : "{{ post.title }}",
	"sub_title"    : "{{ post.sub_title }}",
	"blurb"    : "{{ post.blurb }}",
	"rating"    : "{{ post.rating }}",
	"tags"     : [{% for tag in post.tags %}"{{ tag }}",{% endfor %} null],
	"brewery"     : "{{ post.brewery }}",
	"beer_name"     : "{{ post.beer_name }}",
	"beer_abv"     : "{{ post.beer_abv }}",
	"beer_ibus"     : "{{ post.beer_ibus }}",
	"beer_type"     : "{{ post.beer_type }}",
	"href"     : "{{ post.url }}",
	"created"     : "{{ post.date | date_to_string }}"
}